		{
			"name": "products",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"let posted = [];",
							"try { posted = JSON.parse(pm.environment.get(\"MY_PRODUCTS\") || \"[]\"); } catch (e) { posted = []; }",
							"",
							"// Om inget postat ännu: informativt, inte fail",
							"pm.test(\"Har postade produkter att verifiera\", function () {",
							"  pm.expect(posted.length, \"Du har inte sparat några produkter i MY_PRODUCTS. Kör POST /products först.\").to.be.greaterThan(0);",
							"});",
							"",
							"pm.test(\"New product contains an id which is a number\", function () {",
							"    const jsonData = pm.response.json();",
							"    const newProduct = jsonData[jsonData.length - 1]; // Assuming the new product is the last in the array",
							"    pm.expect(newProduct).to.have.property(\"id\");",
							"    pm.expect(newProduct.id).to.be.a(\"number\");",
							"",
							"    ",
							"});pm.test(\"Response time is less than 200ms\", function () {",
							"",
							"});pm.test(\"All products are shown\", function () {",
							"    const jsonData = pm.response.json();",
							"    pm.expect(jsonData).to.be.an(\"array\").that.is.not.empty;",
							"    jsonData.forEach(product => {",
							"        pm.expect(product).to.have.property(\"id\");",
							"        pm.expect(product.id).to.be.a(\"number\");",
							"        pm.expect(product).to.have.property(\"name\");",
							"        pm.expect(product.name).to.be.a(\"string\");",
							"    });",
							"});    pm.expect(pm.response.responseTime).to.be.below(200);",
							"",
							"pm.test(\"Response body is not empty\", function () {",
							"    pm.expect(pm.response.text()).to.not.be.empty;",
							"});",
							"pm.test(\"Response body contains one product\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.length).to.equal(21);",
							"});",
							"pm.test(\"Response body contains correct product name\", function () {",
							"    var json = pm.response.json()[0]; // Define json correctly",
							"    pm.expect(json).to.have.property(\"name\");",
							"    pm.expect(json.name).to.be.a(\"string\");",
							"    ",
							"});",
							"pm.test(\"Response body contains correct product id\", function () {",
							"    var json = pm.response.json()[0]; // Define json correctly",
							"    pm.expect(json).to.have.property(\"id\");",
							"    pm.expect(json.id).to.be.a(\"number\");",
							"",
							"});"
